---
title: Projects | Mickey Loreti
---

<div class="project-grid">
  <% data.projects.projects.each do |project| %>
    <div class="project">
      <a href="/projects/<%= project.path %>">
        <div class="thumb" style="background-image: url(/images/<%= project.thumb %>)"></div>
        <div class="overlay">
          <h3><%= project.title %> </h3>
          <p><%= project.descrpition %></p>
        </div>
      </a>
    </div>
  <% end %>
</div>
